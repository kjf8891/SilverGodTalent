<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <Button
                android:id="@+id/gotoHelpMenuBtn"
                android:onClick="gotoHelpMenu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/help"/>
            <Button
                android:id="@+id/gotoRecruitMenuBtn"
                android:onClick="gotoRecruitMenu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/recruit"/>
            <Button
                android:id="@+id/gotoMentoringMenuBtn"
                android:onClick="gotoMentoringMenu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/mentoring"/>
            <Button
                android:id="@+id/gotoClubMenuBtn"
                android:onClick="gotoClubMenu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/club"/>
            <Button
                android:id="@+id/gotoMyPageMenuBtn"
                android:onClick="gotoMyPageMenuBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/mypage"/>
        </LinearLayout>




    </FrameLayout>

</LinearLayout>
